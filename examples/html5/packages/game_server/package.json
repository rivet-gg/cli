{
  "name": "rivet-example-html5-game-server",
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "dev": "npx nodemon --watch 'src/**/*.ts' --exec 'npx ts-node' src/index.ts",
    "install": "exit 0 && (cd ../sdk && yarn) && rm -rf node_modules/rivet-sdk && cp -r ../sdk node_modules/rivet-sdk",
    "build": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/node-fetch": "^2.6.11",
    "@types/ws": "^8.5.12",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "node-fetch": "2",
    "ws": "^7.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "nodemon": "^2.0.20",
    "rivet-sdk": "*",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
