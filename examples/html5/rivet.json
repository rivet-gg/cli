{
	"sdks": [{"output": "packages/rivet_sdk", "target": "typescript"}],
	"runtime": {
		"cors": {
			"origins": ["http://127.0.0.1:8080", "http://localhost:8080"]
		}
	},
	"registries": {
		"local": {
			"local": {
				"directory": "../../../modules/modules"
			}
		}
	},
	"modules": {
		"rate_limit": {
			"registry": "local"
		},
		"tokens": {
			"registry": "local"
		},
		"lobbies": {
			"registry": "local",
			"config": {
				"lobbies": {
					"regions": ["atl"],
					"backend": {
						"server": {
							"environment": {
								"SERVER_HOSTNAME": "0.0.0.0"
							},
							"ports": {
								"game": {
									"protocol": "https",
									"internalPort": 7778
								}
							},
							"resources": {
								"cpu": 250,
								"memory": 250
							}
						}
					}
				}
			}
		},
		"rivet": {
			"registry": "local"
		}
	}
}
